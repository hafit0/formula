#include <iostream>
#include <math.h>

using namespace std;

int main()
{
    float a,b,c;
    cout << "input nilai a : " ;
    cin>>a;
    cout << "input nilai b : ";
    cin>>b;
    cout << "input nilai c : ";
    cin>>c;
    cout<<"maka hasil dari perhitungan dengan formula adalah " <<(2*a+b)/(4*c-6);


    return 0;
}
